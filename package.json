{
  "name": "apicBI",
  "version": "0.0.2",
  "scripts": {
    "init": "node init.js -dataset ApicBI",
    "reinit": "node init.js -dataset ApicBI -reinit 1",
    "clean": "node clean.js -dataset ApicBI",
    "clear_history": "node clear_file.js -filepath assets/lastUpdated.json",
    "clear_token": "node clear_file.js -filepath assets/token.json",
    "clear_dataset": "node clear_file.js -filepath assets/datasets_ApicBI.json",
    "clear_session": "npm run clear_history && npm run clear_token && npm run clear_dataset",
    "reset": "npm run clean && npm run init && npm run clear_session",
    "hard_reset": "npm run clean && npm run reinit && npm run clear_session",
    "serve": "forever -c node server.js",
    "init_icws": "node init.js -dataset ApicBI_ICWS",
    "reinit_icws": "node init.js -dataset ApicBI_ICWS -reinit 1",
    "clean_icws": "node clean.js -dataset ApicBI_ICWS",
    "clean_pushed_icws": "node clean_pushed.js",
    "clear_dataset_icws": "node clear_file.js -filepath assets/datasets_ApicBI_ICWS.json",
    "clear_session_icws": "npm run clear_token && npm run clear_dataset_icws",
    "reset_icws": "npm run clean_icws && npm run init && npm run clear_session_icws && npm run clean_pushed_icws",
    "hard_reset_icws": "npm run clean_icws && npm run reinit_icws && npm run clear_session_icws && npm run clean_pushed_icws",
    "build:modules": "webpack",
    "deploy:modules": "webpack --optimize-minimize"
  },
  "description": "A communication layer between ICWS and Power BI",
  "main": "index.js",
  "dependencies": {
    "adal-node": "0.1.17",
    "azure": "0.10.6",
    "bluebird": "3.0.6",
    "body-parser": "1.15.2",
    "chalk": "1.1.1",
    "express": "4.14.0",
    "flowtype": "1.0.0",
    "jquery": "3.1.0",
    "later": "1.2.0",
    "lodash": "4.6.1",
    "lokijs": "1.4.1",
    "moment": "2.11.1",
    "morgan": "1.7.0",
    "node-env-file": "0.1.8",
    "pusher": "1.5.0",
    "pusher-js": "3.2.1",
    "request": "2.67.0",
    "rethinkdb": "2.3.2",
    "seriate": "0.5.4",
    "tiny-eventer": "0.0.1",
    "vue": "1.0.26",
    "vue-resource": "0.9.3",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "css-loader": "0.24.0",
    "gulp": "3.9.1",
    "gulp-livereload": "3.8.1",
    "node-sass": "3.8.0",
    "postcss-loader": "0.11.1",
    "raw-loader": "0.5.1",
    "sass-loader": "4.0.0",
    "shelljs": "0.7.3",
    "style-loader": "0.13.1",
    "webpack": "1.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kugghuset/apicBI.git"
  },
  "author": "Hannes Dahlberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kugghuset/apicBI/issues"
  }
}