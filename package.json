{
  "name": "apicBI",
  "version": "0.0.2",
  "scripts": {
    "init": "node init.js -dataset ApicBI",
    "reinit": "node init.js -dataset ApicBI -reinit 1",
    "clean": "node clean.js -dataset ApicBI",
    "clear_history": "node clear_file.js -filepath assets/lastUpdated.json",
    "clear_token": "node clear_file.js -filepath assets/token.json",
    "clear_dataset": "node clear_file.js -filepath assets/datasets_ApicBI.json",
    "clear_session": "npm run clear_history && npm run clear_token && npm run clear_dataset",
    "reset": "npm run clean && npm run init && npm run clear_session",
    "hard_reset": "npm run clean && npm run reinit && npm run clear_session",
    "serve": "forever -c node server.js"
  },
  "description": "A communication layer between ICWS and Power BI",
  "main": "index.js",
  "dependencies": {
    "adal-node": "0.1.17",
    "azure": "0.10.6",
    "bluebird": "3.0.6",
    "chalk": "1.1.1",
    "express": "4.14.0",
    "later": "1.2.0",
    "lodash": "4.6.1",
    "lokijs": "1.4.1",
    "moment": "2.11.1",
    "node-env-file": "0.1.8",
    "request": "2.67.0",
    "rethinkdb": "2.3.2",
    "seriate": "0.5.4"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-livereload": "3.8.1",
    "shelljs": "0.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kugghuset/apicBI.git"
  },
  "author": "Hannes Dahlberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kugghuset/apicBI/issues"
  }
}